{
  "name": "netflix",
  "version": "0.0.0",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint:fix": "tsc --noEmit && eslint . --ext js,ts --quiet --fix",
    "lint": "tsc --noEmit && eslint . --ext js,ts",
    "e2e": "ng e2e",
    "start": "ng serve --open",
    "dev:ssr": "ng run netflix:serve-ssr",
    "serve:ssr": "node dist/netflix/server/main.js",
    "build:ssr": "ng build --prod && ng run netflix:server:production",
    "prerender": "ng run netflix:prerender",
    "json:users": "json-server --watch ./api/users.json"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/setup-jest.ts"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.2.0",
    "@angular/common": "~10.2.0",
    "@angular/compiler": "~10.2.0",
    "@angular/core": "~10.2.0",
    "@angular/forms": "~10.2.0",
    "@angular/platform-browser": "~10.2.0",
    "@angular/platform-browser-dynamic": "~10.2.0",
    "@angular/platform-server": "~10.2.0",
    "@angular/router": "~10.2.0",
    "@nguniversal/express-engine": "^10.1.0",
    "angular2-materialize-v1": "^1.0.6",
    "express": "^4.15.2",
    "materialize-css": "^1.0.0-rc.2",
    "msw": "^0.26.2",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.0",
    "@angular/cli": "~10.2.0",
    "@angular/compiler-cli": "~10.2.0",
    "@nguniversal/builders": "^10.1.0",
    "@testing-library/angular": "^10.3.1",
    "@testing-library/user-event": "^12.7.1",
    "@types/express": "^4.17.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "codelyzer": "^6.0.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-jsx-a11y": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-unused-imports": "^0.1.3",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "jest-preset-angular": "^8.3.2",
    "json-server": "^0.16.3",
    "ng-bullet": "^1.0.3",
    "prettier": "^2.1.2",
    "prettier-eslint": "^11.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
}
